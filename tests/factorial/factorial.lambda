func print_int(x);
func input_int();

func mul(a, b) {
    if a == 0 {
        return 0;
    } else if a == 1 {
        return b;
    } else {
        return b + mul(a - 1, b);
    }
}

func factorial(n) {
    if n == 0 {
        return 1;
    } else {
        return mul(n, factorial(n - 1));
    }
}

func main() {
    print_int(factorial(input_int()));
}
